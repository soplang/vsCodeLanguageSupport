// Test file for operator parsing issues

qor("Testing operator parsing:")

// Test comparison operators in assignments
abn score = 85
qor("score = " + score)

// This should now work without parentheses
door is_high_score = score >= 80
qor("is_high_score = " + is_high_score)

// Test with parentheses too
door is_passing = (score >= 60)
qor("is_passing = " + is_passing)

// Test with dooro statement
qor("\nTesting dooro with comparison:")

// This should now work directly with a comparison
dooro (score >= 80) {
  xaalad run {
    qor("High score achieved!")
  }
  xaalad been {
    qor("Keep practicing")
  }
}

// Test complex conditions
door complex_condition = (score > 60 && score < 90)
qor("complex_condition = " + complex_condition)

// Test list with comparisons
teed test_list = [score > 80, score < 90, score == 85]
qor("test_list = " + test_list)

// Test object with comparisons
walax test_obj = {
  high_score: score >= 80,
  passing: score >= 60,
  perfect: score == 100
}
qor("test_obj.high_score = " + test_obj.high_score)
qor("test_obj.perfect = " + test_obj.perfect)

qor("\nTest completed")
